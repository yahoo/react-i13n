{
  "presets":[
     "@babel/preset-env",
     [
        "@babel/preset-react",
        {
           "runtime":"automatic"
        }
     ],
     "@babel/preset-typescript"
  ],
  "plugins":[
     "dynamic-import-node",
     "syntax-dynamic-import",
     "@babel/plugin-proposal-class-properties",
     "@babel/plugin-proposal-nullish-coalescing-operator",
     "@babel/plugin-proposal-object-rest-spread",
     "@babel/plugin-proposal-optional-chaining",
     "@babel/plugin-transform-spread"
  ],
  "env":{
     "dist":{
        "presets":[
           [
              "@babel/preset-env",
              {
                 "loose":true
              }
           ],
           [
              "@babel/preset-react",
              {
                 "runtime":"automatic"
              }
           ],
           "@babel/preset-typescript"
        ]
     },
     "dist-es":{
        "presets":[
           [
              "@babel/preset-env",
              {
                 "modules":false,
                 "targets":{
                    "esmodules":true
                 }
              }
           ],
           [
              "@babel/preset-react",
              {
                 "runtime":"automatic"
              }
           ],
           "@babel/preset-typescript"
        ]
     }
  }
}